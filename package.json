{
  "name": "@qualisero/pi-agent-scip",
  "version": "0.1.0",
  "description": "SCIP code intelligence tools for pi-coding-agent",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hook": {
      "types": "./dist/hook.d.ts",
      "import": "./dist/hook.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "pi-agent-scip-status": "dist/cli/status.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w --preserveWatchOutput",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "pi",
    "scip",
    "code-intelligence",
    "python"
  ],
  "author": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.46",
    "@sourcegraph/scip-python": "^0.6.6",
    "@sourcegraph/scip-typescript": "^0.4.0"
  },
  "devDependencies": {
    "@mariozechner/pi-coding-agent": "^0.33.0",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@mariozechner/pi-coding-agent": ">=0.33.0"
  }
}
